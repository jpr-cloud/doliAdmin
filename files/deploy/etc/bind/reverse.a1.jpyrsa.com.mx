;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	localhost. root.localhost. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	NS  ns1withX.jpyrsa.com.mx.
@	IN	A	116.203.80.120
@	IN	AAAA	::1

@       IN      NS      ns1withX.jpyrsa.com.mx.
@       IN      A       116.203.80.120
ns1     IN      A       116.203.80.120